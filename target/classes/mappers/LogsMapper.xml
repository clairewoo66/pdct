<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="base.common.mapper.LogsMapper">
    <insert id="addLog">
        INSERT INTO t_logs(operatorId,operatorName,method,message,operateTime) VALUES (
        #{operatorId},#{operatorName},#{method},#{message},#{operateTime}
        )
    </insert>
</mapper>